<?xml version='1.0' encoding='UTF-8'?>
<com.tikal.jenkins.plugins.multijob.MultiJobProject>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>SCENARIO_FILE</name>
          <description>The scenario to execute</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>PERF_PLF_INTRANET_00_LOGIN.jmx</string>
              <string>PERF_PLF_INTRANET_01_DISCUSSION_WRITE.jmx</string>
              <string>PERF_PLF_INTRANET_01_PROFILE_READ.jmx</string>
              <string>PERF_PLF_INTRANET_02_DISCUSSION_READ.jmx</string>
              <string>PERF_PLF_INTRANET_02_WIKI_UPDATE_WRITE.jmx</string>
              <string>PERF_PLF_INTRANET_03_DOCUMENT_READ.jmx</string>
              <string>PERF_PLF_INTRANET_04_SPACE_READ.jmx</string>
              <string>PERF_PLF_INTRANET_05_WIKI_READ.jmx</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>USER_DATASET_NAME</name>
          <description>The set of users to use</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>NONE</string>
              <string>PERF_PLF_INTRANET_DBINT1_Users.csv</string>
              <string>PERF_PLF_INTRANET_DBINT2_Users.csv</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>SPACE_DATASET_NAME</name>
          <description>The set of spaces to use</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>NONE</string>
              <string>PERF_PLF_INTRANET_DBINT1_04_SPACE_READ_SpaceLinks.csv</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>TOPIC_DATASET_NAME</name>
          <description>The set ok wiki pages to use</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>NONE</string>
              <string>PERF_PLF_INTRANET_DBINT1_02_DISCUSSION_TopicsLinks.csv</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>WIKI_DATASET_NAME</name>
          <description>The set ok wiki pages to use</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>NONE</string>
              <string>PERF_PLF_INTRANET_DBINT1_05_WIKI_READ_WikiLinks.csv</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>HOST</name>
          <description>The host on which the test is launched</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>tqa-ap.exoplatform.lan</string>
              <string>tqa-as01.exoplatform.lan</string>
              <string>tqa-as02.exoplatform.lan</string>
              <string>tqa-as03.exoplatform.lan</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>PORT</name>
          <description>The port on which the test is launched</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>80</string>
              <string>8080</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>PAGE_RESPONSE_TIME</name>
          <description>-JexpPageResponseTimeout</description>
          <defaultValue>30000</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>VUG_MAX_THREAD</name>
          <description>-JexpVUGMaxThread for tests execution</description>
          <defaultValue>1000</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>tqa-data01</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command># We create a directory where we will put all results
_current_date=`date -u &quot;+%Y%m%d-%H%M%S-UTC&quot;`
_current_results_path=&quot;/mnt/results/${_current_date}-${TQA_LAB}-PLF354-JBOSSEAP511-${SCENARIO_FILE%.jmx}-${VUG_MAX_THREAD}VU-2Nodes&quot;
mkdir -p ${_current_results_path}
rm -f /mnt/results/current
ln -fs ${_current_results_path} /mnt/results/current
</command>
    </hudson.tasks.Shell>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>Start Primary Application server</phaseName>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-start-jboss-as02</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
      </phaseJobs>
      <continuationCondition>SUCCESSFUL</continuationCondition>
    </com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>Start others Application servers nodes</phaseName>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-start-jboss-as03</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
      </phaseJobs>
      <continuationCondition>SUCCESSFUL</continuationCondition>
    </com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>Start JMXTrans</phaseName>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-start-jmxtrans-as02</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-start-jmxtrans-as03</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
      </phaseJobs>
      <continuationCondition>SUCCESSFUL</continuationCondition>
    </com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>Warm UP Server</phaseName>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-launch-jmeter</jobName>
          <jobProperties>SCENARIO_FILE=${SCENARIO_FILE}
USER_DATASET_NAME=${USER_DATASET_NAME}
SPACE_DATASET_NAME=${SPACE_DATASET_NAME}
TOPIC_DATASET_NAME=${TOPIC_DATASET_NAME}
WIKI_DATASET_NAME=${WIKI_DATASET_NAME}
PAGE_RESPONSE_TIME=${PAGE_RESPONSE_TIME}
HOST=${HOST}
PORT=${PORT}
THINK_TIME=20000
THREAD_COUNT=10
LOOP_COUNT=10000000
RAMPUP=10
DURATION=600
VUG_MAX_THREAD_DURATION=0
VUG_MAX_THREAD=0
VUG_WAIT_TIME=0
VUG_UP_STEP=0
VUG_UP_DURATION=0
VUG_UP_RAMPUP=0
VUG_DOWN_STEP=0
VUG_DOWN_DURATION=0
GROUPING_REQUESTS_BY_USERS_LEVEL=false</jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
      </phaseJobs>
      <continuationCondition>SUCCESSFUL</continuationCondition>
    </com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>Execute Tests</phaseName>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-launch-jmeter</jobName>
          <jobProperties>SCENARIO_FILE=${SCENARIO_FILE}
USER_DATASET_NAME=${USER_DATASET_NAME}
SPACE_DATASET_NAME=${SPACE_DATASET_NAME}
TOPIC_DATASET_NAME=${TOPIC_DATASET_NAME}
WIKI_DATASET_NAME=${WIKI_DATASET_NAME}
PAGE_RESPONSE_TIME=${PAGE_RESPONSE_TIME}
VUG_MAX_THREAD=${VUG_MAX_THREAD}
HOST=${HOST}
PORT=${PORT}
THINK_TIME=20000
THREAD_COUNT=0
LOOP_COUNT= 10000000
RAMPUP=10
DURATION=0
VUG_MAX_THREAD_DURATION=180
VUG_WAIT_TIME=15
VUG_UP_STEP=100
VUG_UP_DURATION=180
VUG_UP_RAMPUP=30
VUG_DOWN_STEP=100
VUG_DOWN_DURATION=15
GROUPING_REQUESTS_BY_USERS_LEVEL=true</jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
      </phaseJobs>
      <continuationCondition>SUCCESSFUL</continuationCondition>
    </com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>Stop and Report</phaseName>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-stop-jboss-as02</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-stop-jboss-as03</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-stop-jmxtrans-as02</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-stop-jmxtrans-as03</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-gateinuxp-extract-results</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
      </phaseJobs>
      <continuationCondition>SUCCESSFUL</continuationCondition>
    </com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
  </builders>
  <publishers>
    <hudson.plugins.emailext.ExtendedEmailPublisher>
      <recipientList>$DEFAULT_RECIPIENTS</recipientList>
      <configuredTriggers>
        <hudson.plugins.emailext.plugins.trigger.FailureTrigger>
          <email>
            <recipientList></recipientList>
            <subject>$PROJECT_DEFAULT_SUBJECT</subject>
            <body>$PROJECT_DEFAULT_CONTENT</body>
            <sendToDevelopers>true</sendToDevelopers>
            <sendToRequester>false</sendToRequester>
            <includeCulprits>false</includeCulprits>
            <sendToRecipientList>true</sendToRecipientList>
          </email>
        </hudson.plugins.emailext.plugins.trigger.FailureTrigger>
      </configuredTriggers>
      <contentType>default</contentType>
      <defaultSubject>$DEFAULT_SUBJECT</defaultSubject>
      <defaultContent>$DEFAULT_CONTENT</defaultContent>
      <attachmentsPattern></attachmentsPattern>
      <presendScript></presendScript>
    </hudson.plugins.emailext.ExtendedEmailPublisher>
  </publishers>
  <buildWrappers/>
</com.tikal.jenkins.plugins.multijob.MultiJobProject>