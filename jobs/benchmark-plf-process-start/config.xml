<?xml version='1.0' encoding='UTF-8'?>
<com.tikal.jenkins.plugins.multijob.MultiJobProject>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>PRODUCT_VERSION</name>
          <description>The version of Platform to use for the bench (it can be a release or a SNAPSHOT)</description>
          <defaultValue></defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>DATASET</name>
          <description>The dataset to use for the bench</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>DATASET-PLF-3.0.x-Login-Logout-v01</string>
              <string>DATASET-PLF-3.0.x-Social-v01</string>
              <string>DATASET-PLF-3.0.x-Social-v02</string>
              <string>DATASET-PLF-3.0.x-Social-v03</string>
              <string>DATASET-PLF-3.5.x-Login-Logout-v01</string>
              <string>DATASET-PLF-3.5.x-Social-v01</string>
              <string>DATASET-PLF-3.5.x-Social-v02</string>
              <string>DATASET-PLF-3.5.x-Social-v03</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>SCENARIO</name>
          <description>SCENARIO to execute</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>SCENARIO-PLF-3.0.x-AllScenarios-simplified-v01</string>
              <string>SCENARIO-PLF-3.0.x-ECMS-Social-v01</string>
              <string>SCENARIO-PLF-3.0.x-Login-Logout-v01</string>
              <string>SCENARIO-PLF-3.0.x-Social-simplified-v01</string>
              <string>SCENARIO-PLF-3.0.x-Social-v01</string>
              <string>SCENARIO-PLF-3.5.2-qa-website-ECMS-benchs-v01</string>
              <string>SCENARIO-PLF-3.5.x-ECMS-Public-v01</string>
              <string>SCENARIO-PLF-3.5.x-Login-fullrq-v01</string>
              <string>SCENARIO-PLF-3.5.x-Login-Logout-v01</string>
              <string>SCENARIO-PLF-3.5.x-Social-fullrq-v01</string>
              <string>SCENARIO-PLF-3.5.x-Social-v01</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>TARGET_URL</name>
          <description>The (v)host on which the test must be executed
(Should be renamed TARGET HOST)</description>
          <defaultValue>tqa-as01.exoplatform.org</defaultValue>
        </hudson.model.TextParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>TARGET_PORT</name>
          <description>The port on which the test must be executed</description>
          <defaultValue>8080</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>VU</name>
          <description></description>
          <defaultValue>30</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>DURATION</name>
          <description></description>
          <defaultValue>3600</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>RAMPUP</name>
          <description></description>
          <defaultValue>10</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>USERS</name>
          <description></description>
          <defaultValue>99</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>downloads</phaseName>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-plf-task-download-product</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
          <exposedSCM>false</exposedSCM>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>benchmark-plf-task-download-dataset</jobName>
          <jobProperties></jobProperties>
          <currParams>true</currParams>
